import base64

def xor_encrypt(data, key):
    # HZMRISH_Validation_Key1990
    encrypted = bytearray()
    key_len = len(key)
    for i, c in enumerate(data):
        encrypted.append(ord(c) ^ ord(key[i % key_len]))
    return encrypted

def base64_encode(data):
    return base64.b64encode(data).decode('ascii')

# 原始数据
data = {
    "机器码1": "2024-12-31",
    "机器码2": "2024-06-30"
}

key = "HZMRISH_Validation_Key"

encrypted_data = {}
for machine_code, expiry_date in data.items():
    encrypted_machine_code = base64_encode(xor_encrypt(machine_code, key))
    encrypted_expiry_date = base64_encode(xor_encrypt(expiry_date, key))
    encrypted_data[encrypted_machine_code] = encrypted_expiry_date

# 然后将encrypted_data保存为JSON
